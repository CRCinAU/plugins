<form>
    <field>
        <id>abuseipdb.general.enabled</id>
        <label>Service Enabled</label>
        <type>checkbox</type>
        <help>Enable this feature</help>
    </field>
    <field>
        <id>abuseipdb.general.flush_on_start</id>
        <label>Flush Alias entries on start</label>
        <type>checkbox</type>
        <help>Enable this feature</help>
    </field>
    <field>
        <id>abuseipdb.general.api_key</id>
        <label>API Key</label>
        <type>text</type>
        <help><![CDATA[API Key from abuseipdb.com]]></help>
        <hint>API Key from abuseipdb.com</hint>
    </field>
    <field>
        <id>abuseipdb.general.log_interval</id>
        <label>Log Interval</label>
        <type>text</type>
        <help><![CDATA[Log status every X seconds]]></help>
        <hint>Log status every X seconds</hint>
    </field>
</form>
